{
  "name": "gaze",
  "version": "1.0.0",
  "description": "Click-to-code webpack loader for Next.js Turbopack - see exactly where your React components live",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "turbopack",
    "webpack-loader",
    "nextjs",
    "react",
    "source-location",
    "debugging",
    "jsx"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "webpack": ">=5.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@babel/generator": "^7.24.0",
    "schema-utils": "^4.2.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/loader-utils": "^2.0.6",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0"
  }
}